<!DOCTYPE html>
<html>
<head>
  <title>Cmom</title>
  <%= stylesheet_link_tag "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<header>
</header>
<% flash.each do |name, msg| %>
    <%= content_tag :div, :class => "flash #{name}" do %>
        <%= msg %>
    <% end %>
<% end %>
<div id="page" role="main">
  <header>
    <%= link_to 'Web', root_path %>
    <%= link_to 'Participantes', users_path %>
    <%= link_to 'Articulos', articles_path %>
  </header>
  <%= yield %>
</div>
<footer>
</footer>
</body>
</html>
